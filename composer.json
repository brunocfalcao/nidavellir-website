{
    "name": "brunocfalcao/nidavellir-website",
    "description": "Nidavellir website",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/nidavellir-website",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Nidavellir\\Website\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nidavellir\\Website\\WebsiteServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
